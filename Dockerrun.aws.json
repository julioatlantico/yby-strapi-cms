{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "yby-strapi-cms-app",
      "image": "977099021902.dkr.ecr.us-east-1.amazonaws.com/yby-strapi-cms-app/yby-strapi-cms-app:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 1337,
          "containerPort": 1337
        }
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        }
      ]
    }
  ]
}
